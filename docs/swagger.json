    "basePath": "/api/v1",
    "paths": {
        "/namespace/": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Retrieves namespace based on given name",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.Namespace"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Namespace": {
            "type": "object",
            "properties": {
                "name": {
                    "description": "Namespace name\nin: string",
                    "type": "string"
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
